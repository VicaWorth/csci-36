<h2 class="text-light py-2">Learning Outcomes</h2>


<!-- PLOs that are overdue for an update -->
<h3>
    <button type="button" class="collapse-button">High Priority PLOs</button>
</h3>
<table class="table table-striped table-dark collapse-content">
    <thead>
        <th>Outcome ID</th>
        <th>Program</th>
        <th>Last Updated</th>
        <th>Update By</th>
    </thead>
    <tbody>
        <% for (let outcome of PLOs_1) { %>
        <tr><button type="button" class="collapse-button">
            <td><%=outcome.outcome_id%></td>
            <td>
                <a href="/programs/<%=outcome.domain_id%>">
                    <%=outcome.domain_name%>
                </a>
            </td>
            <td><%=outcome.last_updated%></td>
            <td><%=outcome.next_update%></td>
        </button></tr>
        <!-- Collapsible description spans entire row -->
        <tr class="collapse-content">
            <td colspan="0"><%=outcome.outcome_description%></td>
        </tr>
        <% } %>
    </tbody>
</table>


